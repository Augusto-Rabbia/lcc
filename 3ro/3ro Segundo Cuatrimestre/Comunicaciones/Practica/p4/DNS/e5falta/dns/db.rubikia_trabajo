$TTL 2d
$ORIGIN rubikia.com.

@		IN	SOA	dns	hostmaster	(
	2024041400
	12h ; refresh time
	30m ; retry time
	2w  ; expiry time
	2h	; negative TTL
)

		IN	NS	dns
		IN	MX	10	mails
		IN	NS	ns_backup

ddi		IN	NS	dns2.ddi

ns_backup IN A	193.168.7.18

mails	IN	CNAME dns
dns		IN	A	192.168.7.18
www		IN	A	192.168.7.181
flavio	IN 	A	192.168.7.10

