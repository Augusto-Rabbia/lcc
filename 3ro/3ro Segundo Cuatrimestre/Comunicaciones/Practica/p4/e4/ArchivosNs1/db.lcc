$TTL 2d
$ORIGIN lcc.ar.

@ IN SOA ns1 hostmaster (
    12h ; refresh time
    30m ; retry time
    2w  ; expiry time
    2h  ; neg TTL
)

			IN	NS	ns1
			IN	NS	ns2				; definiendo los esclavos
			IN	NS	ns.fceia.ar.	; de ns1. Como fceia.ar no esta en nuestro alcance, no lo metemos en la lista de ipv4s
			IN	MX	10	mx
comunic		IN	NS	ns.comunic
			IN	NS	ns1

ns1			IN	A	192.168.235.1
ns2			IN	A	192.168.235.2
ns.comunic	IN	A	192.168.235.160

; hosts de la red 192.168.235.0/24
